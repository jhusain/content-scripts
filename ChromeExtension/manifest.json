{
  "manifest_version": 2,

  "name": "Content Scripts example",
  "description": "A simple content_scripts example.",
  "version": "1.0",
  "web_accessible_resources": ["logo.svg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["icon_content.js"]
    },
    {
      "matches": ["*://www.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "<all_urls>"]
}
