{
  "manifest_version": 2,

  "name": "Content Scripts example",
  "description": "A simple content_scripts example.",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["*://www.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
